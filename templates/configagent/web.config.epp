<%- | String  $dotnet_argument,
      Boolean $stdout_log_enabled,
      String  $stdout_log_file
| -%>
<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified" />
    </handlers>
    <aspNetCore processPath="dotnet" arguments="<%= $dotnet_argument %>" stdoutLogEnabled="<%= $stdout_log_enabled %>" stdoutLogFile="<%= $stdout_log_file %>" />
  </system.webServer>
</configuration>
